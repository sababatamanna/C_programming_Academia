#include<stdio.h>
int main()
{
int x;

scanf("%d",&x);
switch(x)
{
    case 1: printf("A");
            break;
    case 2: printf("B");
            break;
    case 3: printf("C");
            break;
    case 4: printf("D");
            break;
    case 5: printf("E");
            break;
    default:printf("Error");
           break;
}
}
